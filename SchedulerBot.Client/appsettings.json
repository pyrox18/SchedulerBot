{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss.fff%K} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "SchedulerBotContext": "Server=localhost;Database=schedulerbot",
    "SchedulerBot": "Server=localhost;Database=schedulerbot"
  },
  "Bot": {
    "Token": "BOT_TOKEN_HERE",
    "Prefixes": [ "+" ],
    "Status": "+help | v{0}",
    "Links": {
      "BotInvite": "http://bit.ly/schedulerbot-discord",
      "SupportServer": "https://discord.gg/CRxRn5X",
      "TimezoneList": "http://bit.ly/tz-timezones"
    }
  }
}
